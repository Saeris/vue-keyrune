{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2"
  ],
    "plugins": [
      "vue-jsx-sync",
      "jsx-vue-functional",
      "transform-class-properties",
      "syntax-flow",
      "transform-flow-strip-types",
      "transform-decorators-legacy",
      "transform-vue-jsx",
      "transform-runtime"
    ],
      "env": {
    "test": {
      "presets": [
        "env",
        "stage-2"
      ],
        "plugins": [
          "vue-jsx-sync",
          "jsx-vue-functional",
          "transform-class-properties",
          "syntax-flow",
          "transform-flow-strip-types",
          "transform-vue-jsx",
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
    }
  }
}
